<%= render "shared/header" %>

<div class='form' >
  <div class='lavel' >日付を選択</div>
  <div  class='date-select' >date</div>
  <div  class='lavel' >予定を入力</div>
  <div class='text-input' >plan</div>
  <div>保存</div>
</div>
<div class='calendar'>
  <% 7.times do |day| %>
