      <div class='date'>
        5/5(日)
      </div>
      <ul class='content'>
        <li class='plan-list'>・料理1</li>
        <li class='plan-list'>・料理2</li>
        <li class='plan-list'>・料理3</li>
      </ul>
    </div>
  <% end %>
</div>